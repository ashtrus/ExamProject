@model IEnumerable<ExamProject.Models.SkillExperience>

@{
    ViewBag.Title = "Index";
}
<br />
<br />
<h2>Skills</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Skill.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Experience)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Skill.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Experience)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SkillExperienceId }) |
            @Html.ActionLink("Details", "Details", new { id=item.SkillExperienceId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SkillExperienceId })
        </td>
    </tr>
}

</table>

@Html.Partial("SkillsProgressBar")

